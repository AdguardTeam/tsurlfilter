import ParsedCookie from '../parsed-cookie';
/**
 * Cookie api implementation
 */
export default class BrowserCookieApi {
    /**
     * Removes cookie
     *
     * @param name
     * @param url
     */
    removeCookie(name: string, url: string): Promise<boolean>;
    /**
     * Updates cookie
     *
     * @param cookie Cookie for update
     */
    modifyCookie(cookie: ParsedCookie): Promise<boolean>;
    private static convertToSetDetailsType;
    /**
     * Returns same-site type
     *
     * @param sameSite
     */
    private static getSameSiteStatus;
}
