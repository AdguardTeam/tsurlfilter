import { IAdvancedModifier } from './advanced-modifier';
/**
 * Replace modifier class
 */
export declare class ReplaceModifier implements IAdvancedModifier {
    /**
     * Replace option value
     */
    private readonly replaceOption;
    /**
     * Replace option apply func
     */
    private readonly replaceApply;
    /**
     * Constructor
     *
     * @param value
     */
    constructor(value: string);
    /**
     *
     * @param option
     */
    private static parseReplaceOption;
    /**
     * Replace content
     */
    getValue(): string;
    /**
     * Replace apply function
     */
    getApplyFunc(): (input: string) => string;
}
