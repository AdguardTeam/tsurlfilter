/**
 * Represents a filtering rule
 */
export interface IRule {
    /**
     * Rule's source text
     */
    getText(): string;
    /**
     * ID of the filter list this rule belongs to
     */
    getFilterListId(): number;
}
/**
 * Rule with index
 */
export declare class IndexedRule {
    /**
     * Rule
     */
    rule: IRule;
    /**
     * Index
     */
    index: number;
    /**
     * Constructor
     *
     * @param rule
     * @param index
     */
    constructor(rule: IRule, index: number);
}
/**
 * Rule with storage index
 */
export declare class IndexedStorageRule {
    /**
     * Rule
     */
    rule: IRule;
    /**
     * Index
     */
    index: number;
    /**
     * Constructor
     *
     * @param rule
     * @param index
     */
    constructor(rule: IRule, index: number);
}
