declare const PREFERRED_CONTENT_TYPE = "text/plain";
/**
 * Returns content type error.
 *
 * @returns Error with description of supported content types.
 */
declare const getContentTypeError: () => Error;
/**
 * Checks if the content type is supported.
 *
 * @param contentTypeHeader Content type header.
 *
 * @returns True if supported.
 */
declare const isContentTypeSupported: (contentTypeHeader: string | null) => boolean;
export { getContentTypeError, isContentTypeSupported, PREFERRED_CONTENT_TYPE, };
