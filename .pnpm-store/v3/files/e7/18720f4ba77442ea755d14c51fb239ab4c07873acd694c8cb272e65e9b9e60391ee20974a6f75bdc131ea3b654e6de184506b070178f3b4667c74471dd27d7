import { NetworkRule } from '../../../network-rule';
import { DeclarativeRule } from '../../declarative-rule';
/**
 * Describes abstract error when declarative rule is invalid.
 */
export declare abstract class InvalidDeclarativeRuleError extends Error {
    networkRule: NetworkRule;
    declarativeRule: DeclarativeRule;
    /**
     * Describes abstract error when declarative rule is invalid.
     *
     * @param message Message of error.
     * @param networkRule {@link NetworkRule}.
     * @param declarativeRule {@link DeclarativeRule}.
     */
    constructor(message: string, networkRule: NetworkRule, declarativeRule: DeclarativeRule);
}
