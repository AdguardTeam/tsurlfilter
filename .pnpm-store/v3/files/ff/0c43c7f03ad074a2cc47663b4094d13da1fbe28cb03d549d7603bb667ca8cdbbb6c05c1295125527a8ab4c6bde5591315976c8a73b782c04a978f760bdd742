/**
 * Module with miscellaneous syntax utils exposed in API
 */
export declare class RuleSyntaxUtils {
    private static DUMMY_FILTER_ID;
    /**
     * Checks if rule can be matched by domain
     * @param ruleText
     * @param domain
     */
    static isRuleForDomain(ruleText: string, domain: string): boolean;
    /**
     * Checks if rule can be matched by url
     * @param ruleText
     * @param url
     */
    static isRuleForUrl(ruleText: string, url: string): boolean;
}
