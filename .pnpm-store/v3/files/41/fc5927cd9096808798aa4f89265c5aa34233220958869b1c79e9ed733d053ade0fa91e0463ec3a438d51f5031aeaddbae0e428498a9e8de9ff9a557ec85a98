            .map(function (_a) {
            var id = _a.id, rules = _a.rules;
            return rules.map(function (r) { return ({
                hash: r.hash,
                source: {
                    sourceRuleIndex: r.index,
                    filterId: id,
                },
            }); });
        })
            .flat();
        var rulesHashMap = new RulesHashMap(listOfRulesWithHash);
