                }); };
                deserialized = {
                    id: id,
                    data: data,
                    ruleSetContentProvider: {
                        loadSourceMap: function () { return __awaiter(_this, void 0, void 0, function () {
                            var sourceMapRaw, sources;
                            return __generator(this, function (_a) {
                                switch (_a.label) {
                                    case 0: return [4 /*yield*/, getLazyData()];
                                    case 1:
