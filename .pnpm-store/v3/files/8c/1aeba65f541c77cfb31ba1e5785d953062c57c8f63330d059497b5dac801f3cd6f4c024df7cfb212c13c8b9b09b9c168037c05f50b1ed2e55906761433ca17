import { WebRequest } from 'webextension-polyfill';
import HttpHeadersItemType = WebRequest.HttpHeadersItemType;
import HttpHeaders = WebRequest.HttpHeaders;
/**
 * Finds header object by header name (case insensitive)
 *
 * @param headers Headers collection
 * @param headerName Header name
 * @returns header value
 */
export declare function findHeaderByName(headers: HttpHeaders, headerName: string): HttpHeadersItemType | null;
/**
 * Removes header from headers by name
 *
 * @param {Array} headers
 * @param {String} headerName
 * @return {boolean} True if header were removed
 */
export declare function removeHeader(headers: HttpHeaders, headerName: string): boolean;
