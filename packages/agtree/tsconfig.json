{
    "extends": "./tsconfig.base.json",
    "compilerOptions": {
        "noEmit": true,
    },
    "include": [
        ".eslintrc.cjs",
        "jest.config.ts",
        "rollup.config.ts",
        "scripts/**/*.ts",
        "scripts/.eslintrc.cjs",
        "src/compatibility-tables/extractors/.eslintrc.cjs",
        "src/**/*.ts",
        "test/**/*.ts",
        "rollup.plugins.ts",
        "rollup.config.ts",
    ],
    "exclude": [
        "./node_modules",
        "./dist",
        "./test/smoke"
    ],
}
