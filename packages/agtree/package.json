{
    "name": "@adguard/agtree",
    "version": "2.1.0",
    "description": "Universal adblock filter list parser.",
    "keywords": [
        "adblock",
        "filter",
        "list",
        "parser",
        "ast"
    ],
    "author": "Adguard Software Ltd.",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/AdguardTeam/tsurlfilter.git",
        "directory": "packages/agtree"
    },
    "bugs": {
        "url": "https://github.com/AdguardTeam/tsurlfilter/issues"
    },
    "homepage": "https://github.com/AdguardTeam/tsurlfilter/tree/master/packages/agtree#readme",
    "main": "dist/agtree.js",
    "module": "dist/agtree.mjs",
    "types": "dist/agtree.d.ts",
    "exports": {
        ".": {
            "types": "./dist/agtree.d.ts",
            "import": "./dist/agtree.mjs",
            "require": "./dist/agtree.js"
        }
    },
    "files": [
        "dist"
    ],
    "engines": {
        "node": ">=17"
    },
    "scripts": {
        "build": "pnpm clean && pnpm build-txt && pnpm build-compatibility-tables && pnpm build-compatibility-tables-wiki && pnpm rollup --config rollup.config.ts --configPlugin typescript && rimraf dist/types",
        "build-txt": "tsx scripts/build-txt.ts",
        "build-compatibility-tables": "tsx scripts/build-compatibility-tables.ts",
        "build-compatibility-tables-wiki": "tsx scripts/build-compatibility-tables-wiki.ts",
        "check-types": "tsc --noEmit",
        "clean": "rimraf dist",
        "coverage": "jest --runInBand --coverage",
        "increment": "pnpm version patch --no-git-tag-version",
        "lint": "pnpm check-types && pnpm lint:ts && pnpm lint:md",
        "lint:md": "markdownlint .",
        "lint:ts": "eslint . --cache --ext .ts",
        "precommit": "pnpm check-types && pnpm lint && pnpm test",
        "test": "jest --runInBand",
        "test:smoke": "(cd test/smoke/esm && pnpm test) && (cd test/smoke/cjs && pnpm test) && (cd test/smoke/typescript && pnpm test)"
    },
    "devDependencies": {
        "@rollup/plugin-alias": "^5.0.0",
        "@rollup/plugin-commonjs": "^25.0.2",
        "@rollup/plugin-json": "^6.0.0",
        "@rollup/plugin-node-resolve": "^15.1.0",
        "@rollup/plugin-swc": "^0.4.0",
        "@rollup/plugin-typescript": "^11.1.2",
        "@swc/core": "^1.7.24",
        "@swc/helpers": "^0.5.3",
        "@swc/jest": "^0.2.29",
        "@types/benchmark": "^2.1.5",
        "@types/clone-deep": "^4.0.1",
        "@types/fs-extra": "^11.0.4",
        "@types/jest": "^29.5.12",
        "@types/js-yaml": "^4.0.5",
        "@types/node": "^20.2.3",
        "@types/semver": "^7.5.0",
        "@types/sprintf-js": "^1.1.4",
        "@typescript-eslint/eslint-plugin": "^5.60.1",
        "@typescript-eslint/parser": "^5.60.1",
        "benchmark": "^2.1.4",
        "camelcase-keys": "^7.0.2",
        "consola": "^3.2.3",
        "console-table-printer": "^2.12.0",
        "date-fns": "^3.3.1",
        "eslint": "^8.43.0",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-import-newlines": "^1.3.1",
        "eslint-plugin-jsdoc": "^46.4.0",
        "fs-extra": "^11.2.0",
        "glob": "^9.3.2",
        "jest": "^29.5.0",
        "js-yaml": "^4.1.0",
        "jsdoc": "^4.0.2",
        "lodash": "4.17.21",
        "markdown-table": "^3.0.3",
        "markdownlint": "^0.29.0",
        "markdownlint-cli": "^0.35.0",
        "node-fetch": "^3.3.2",
        "object-sizeof": "^2.6.4",
        "playwright": "^1.41.2",
        "prettier": "^3.3.2",
        "regenerator-runtime": "^0.14.0",
        "rimraf": "^5.0.1",
        "rollup": "^3.25.3",
        "rollup-plugin-dts": "^5.3.0",
        "rollup-plugin-node-externals": "^6.1.1",
        "systeminformation": "^5.22.0",
        "ts-node": "^10.9.2",
        "tsx": "^4.7.3",
        "type-fest": "^4.18.0",
        "typescript": "^5.0.4"
    },
    "dependencies": {
        "@adguard/css-tokenizer": "workspace:^",
        "clone-deep": "^4.0.1",
        "is-ip": "3.1.0",
        "json5": "^2.2.3",
        "semver": "^7.5.3",
        "sprintf-js": "^1.1.3",
        "tldts": "^5.7.112",
        "xregexp": "^5.1.1",
        "zod": "3.21.4"
    }
}
