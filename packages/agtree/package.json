{
    "name": "@adguard/agtree",
    "version": "1.0.0",
    "description": "Universal adblock filter list parser.",
    "keywords": [
        "adblock",
        "filter",
        "list",
        "parser",
        "ast"
    ],
    "author": "AdGuard Software Ltd.",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/AdguardTeam/AGTree.git"
    },
    "bugs": {
        "url": "https://github.com/AdguardTeam/AGTree/issues"
    },
    "homepage": "https://github.com/AdguardTeam/AGTree#readme",
    "type": "module",
    "main": "dist/agtree.cjs",
    "module": "dist/agtree.esm.js",
    "browser": "dist/agtree.umd.min.js",
    "types": "dist/agtree.d.ts",
    "files": [
        "dist"
    ],
    "engines": {
        "node": ">=14"
    },
    "scripts": {
        "build": "yarn clean && yarn build-types && yarn rollup --config rollup.config.ts --configPlugin typescript && yarn clean-types",
        "build-types": "tsc --declaration --emitDeclarationOnly --outdir dist/types",
        "check-types": "tsc --noEmit",
        "clean": "rimraf dist",
        "clean-types": "rimraf dist/types",
        "coverage": "jest --runInBand --coverage",
        "lint": "eslint . --ext .ts",
        "precommit": "yarn check-types && yarn lint && yarn test",
        "test": "jest --runInBand"
    },
    "devDependencies": {
        "@babel/core": "^7.20.5",
        "@babel/preset-env": "^7.20.2",
        "@rollup/plugin-alias": "^5.0.0",
        "@rollup/plugin-babel": "^6.0.3",
        "@rollup/plugin-commonjs": "^25.0.0",
        "@rollup/plugin-json": "^6.0.0",
        "@rollup/plugin-node-resolve": "^15.0.2",
        "@rollup/plugin-terser": "^0.4.0",
        "@rollup/plugin-typescript": "^11.1.1",
        "@swc/core": "^1.3.59",
        "@swc/jest": "^0.2.26",
        "@types/clone-deep": "^4.0.1",
        "@types/jest": "^29.5.1",
        "@types/node": "^20.2.1",
        "@types/semver": "^7.3.13",
        "@typescript-eslint/eslint-plugin": "^5.27.0",
        "@typescript-eslint/parser": "^5.27.0",
        "eslint": "^8.24.0",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-import-newlines": "^1.3.1",
        "eslint-plugin-jsdoc": "^44.2.4",
        "jest": "^29.1.2",
        "jsdoc": "^4.0.0",
        "rimraf": "^5.0.1",
        "rollup": "^3.2.3",
        "rollup-plugin-dts": "^5.0.0",
        "rollup-plugin-node-externals": "^5.1.3",
        "rollup-plugin-polyfill-node": "^0.12.0",
        "ts-jest": "^29.0.3",
        "ts-node": "^10.9.1",
        "typescript": "^5.0.4"
    },
    "dependencies": {
        "@adguard/ecss-tree": "^1.0.8",
        "clone-deep": "^4.0.1",
        "json5": "^2.2.1",
        "semver": "^7.3.8",
        "tldts": "^5.7.112"
    }
}
