{
  "name": "@adguard/api",
  "version": "2.0.0",
  "description": "This is a TypeScript library that implements AdGuard's extension API",
  "main": "dist/index.js",
  "typings": "dist/types/background/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./content-script": "./dist/content-script.js"
  },
  "typesVersions": {
    "*": {
      "content-script": [
        "dist/types/content-script/index.d.ts"
      ]
    }
  },
  "scripts": {
    "lint": "eslint 'src/**/*.ts'",
    "build": "webpack --mode=production && yarn types",
    "types": "tsc --project tsconfig.build.json --declaration --emitDeclarationOnly --outdir dist/types"
  },
  "author": "Adguard",
  "license": "GPL-3.0-only",
  "dependencies": {
    "@adguard/assistant": "^4.3.63",
    "@adguard/filters-downloader": "^1.1.13",
    "@adguard/tsurlfilter": "^1.0.42",
    "@adguard/tswebextension": "^0.0.25",
    "url": "^0.11.0",
    "webextension-polyfill": "0.10.0",
    "zod": "3.19.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-typescript": "^7.18.6",
    "@types/webextension-polyfill": "0.9.1",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "5.40.1",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "eslint": "8.25.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.14",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-webpack-plugin": "^3.2.0",
    "prettier": "^2.7.1",
    "terser-webpack-plugin": "^5.3.6",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
}
