{
  "name": "@adguard/tsurlfilter",
  "version": "2.0.0-alpha.31",
  "description": "This is a TypeScript library that implements AdGuard's content blocking rules",
  "main": "dist/tsurlfilter.umd.js",
  "module": "dist/es/index.js",
  "iife": "dist/tsurlfilter.iife.js",
  "typings": "dist/types/src",
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": "./dist/tsurlfilter.umd.js",
    "./cli": "./dist/cli.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/types/src"
      ],
      "cli": [
        "dist/types/cli/index.d.ts"
      ]
    }
  },
  "bin": {
    "tsurlfilter": "dist/cli.js"
  },
  "author": "AdGuard",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AdguardTeam/tsurlfilter.git"
  },
  "license": "GPL-3.0-only",
  "engines": {
    "node": ">=6.0.0"
  },
  "bugs": {
    "url": "https://github.com/AdguardTeam/tsurlfilter/issues"
  },
  "homepage": "https://github.com/AdguardTeam/tsurlfilter#readme",
  "scripts": {
    "lint": "eslint 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rimraf dist && yarn build:types",
    "build": "rollup -c rollup.config.ts",
    "postbuild": "node tasks/build-txt",
    "build:types": "tsc --declaration --emitDeclarationOnly --outdir dist/types",
    "docs": "typedoc src --out docs",
    "start": "rollup -c rollup.config.ts -w",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:prod": "yarn lint && yarn test --no-cache --runInBand",
    "test:debug": "node --inspect node_modules/.bin/jest --watch --runInBand",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "precommit": "lint-staged",
    "prepublishOnly": "yarn build",
    "increment": "yarn version --patch --no-git-tag-version",
    "increment:alpha": "yarn version --prerelease --no-git-tag-version",
    "examples": "node -r esbuild-register tasks/generate-examples"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "eslint"
    ]
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverageFrom": [
      "src/*.{js,ts}",
      "src/*/*.{js,ts}",
      "src/*/*/*.{js,ts}"
    ],
    "setupFiles": [
      "jest-webextension-mock"
    ]
  },
  "dependencies": {
    "@adguard/scriptlets": "^1.6.55",
    "commander": "9.4.1",
    "ip6addr": "0.2.3",
    "is-cidr": "4.0.2",
    "is-ip": "3.1.0",
    "lru_map": "0.4.1",
    "netmask": "2.0.2",
    "punycode": "2.1.1",
    "tldts": "5.6.45",
    "zod": "3.19.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "22.0.2",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "13.3.0",
    "@rollup/plugin-typescript": "8.3.4",
    "@types/ip6addr": "0.2.3",
    "@types/jest": "28.1.6",
    "@types/netmask": "1.0.30",
    "@types/node": "18.7.1",
    "@types/punycode": "2.1.0",
    "@types/rollup-plugin-node-globals": "1.4.1",
    "@typescript-eslint/eslint-plugin": "5.33.0",
    "@typescript-eslint/parser": "5.33.0",
    "coveralls": "3.1.1",
    "esbuild": "0.14.54",
    "esbuild-register": "3.3.3",
    "eslint": "8.21.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-import-newlines": "1.2.3",
    "eslint-plugin-jsdoc": "39.3.6",
    "jest": "28.1.3",
    "jest-webextension-mock": "3.7.22",
    "lint-staged": "13.0.3",
    "lodash": "4.17.21",
    "rimraf": "3.0.2",
    "rollup": "2.77.3",
    "rollup-plugin-cleanup": "3.2.1",
    "rollup-plugin-node-globals": "1.4.0",
    "rollup-plugin-polyfill-node": "0.10.2",
    "rollup-plugin-preserve-shebangs": "0.2.0",
    "rollup-plugin-terser": "7.0.2",
    "ts-jest": "28.0.7",
    "ts-node": "10.9.1",
    "tslib": "2.4.0",
    "typedoc": "0.23.10",
    "typescript": "4.7.4"
  }
}
