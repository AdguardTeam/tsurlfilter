{
  "name": "@adguard/tsurlfilter",
  "version": "3.0.5",
  "description": "This is a TypeScript library that implements AdGuard's content blocking rules",
  "files": [
    "dist"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "typesVersions": {
    "*": {
      "*": ["dist/types/index.d.ts"],
      "cli": ["dist/types/cli/index.d.ts"]
    }
  },
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js"
    },
    "./cli": {
      "types": "./dist/types/cli/index.d.ts",
      "import": "./dist/esm/cli/index.mjs",
      "require": "./dist/cjs/cli/index.js"
    }
  },
  "bin": {
    "tsurlfilter": "dist/cli.js"
  },
  "author": "Adguard Software Ltd.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AdguardTeam/tsurlfilter.git",
    "directory": "packages/tsurlfilter"
  },
  "license": "GPL-3.0-only",
  "engines": {
    "node": ">=6.0.0"
  },
  "bugs": {
    "url": "https://github.com/AdguardTeam/tsurlfilter/issues"
  },
  "homepage": "https://github.com/AdguardTeam/tsurlfilter/tree/master/packages/tsurlfilter#readme",
  "scripts": {
    "lint": "eslint 'src/**/*.ts' 'src/cli/**/*.ts' 'test/**/*.ts' && pnpm lint:tsc",
    "lint:tsc": "tsc --noEmit -p test/tsconfig.tests.json",
    "check-types": "pnpm lint:tsc",
    "prebuild": "rimraf dist",
    "build": "pnpm prebuild && rollup -c rollup.config.ts --configPlugin @rollup/plugin-typescript --bundleConfigAsCjs && pnpm postbuild",
    "postbuild": "node tasks/build-txt",
    "docs": "typedoc src --out docs",
    "start": "rollup -c rollup.config.ts -w",
    "test": "jest --coverage",
    "test:light": "jest --testPathIgnorePatterns=benchmark",
    "test:watch": "jest --coverage --watch",
    "test:smoke": "cd test/builders/rollup-ts && pnpm test",
    "test:prod": "pnpm lint && pnpm test:smoke && pnpm test -- --no-cache --runInBand",
    "test:debug": "node --inspect node_modules/.bin/jest --watch --runInBand",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "precommit": "lint-staged",
    "prepublishOnly": "pnpm build",
    "increment": "pnpm version patch --no-git-tag-version",
    "examples": "node -r esbuild-register tasks/generate-examples"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "eslint"
    ]
  },
  "dependencies": {
    "@adguard/agtree": "workspace:^",
    "@adguard/css-tokenizer": "workspace:^",
    "@adguard/scriptlets": "^1.12.1",
    "cidr-tools": "^6.4.1",
    "commander": "9.4.1",
    "is-cidr": "4.0.2",
    "is-ip": "3.1.0",
    "lru_map": "0.4.1",
    "punycode": "2.1.1",
    "tldts": "5.6.45",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-typescript": "^12.1.1",
    "@swc/core": "^1.7.40",
    "@swc/jest": "^0.2.36",
    "@types/ip6addr": "0.2.3",
    "@types/jest": "29.5.3",
    "@types/lodash": "^4.17.0",
    "@types/netmask": "1.0.30",
    "@types/node": "^22.8.2",
    "@types/punycode": "2.1.0",
    "@types/rollup-plugin-node-globals": "1.4.1",
    "@typescript-eslint/eslint-plugin": "5.59.11",
    "@typescript-eslint/parser": "5.59.11",
    "coveralls": "3.1.1",
    "esbuild": "^0.19.3",
    "esbuild-register": "^3.5.0",
    "escape-string-regexp": "^5.0.0",
    "eslint": "8.21.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-import-newlines": "1.2.3",
    "eslint-plugin-jsdoc": "39.3.6",
    "jest": "^29.6.2",
    "lint-staged": "13.0.3",
    "lodash": "4.17.21",
    "rimraf": "3.0.2",
    "rollup": "^4.24.2",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "tldts-core": "^6.1.57",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.0",
    "typedoc": "^0.26.10",
    "typescript": "^5.6.3"
  }
}
