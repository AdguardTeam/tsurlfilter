{
    "compilerOptions": {
      "module": "ESNext",
      "moduleResolution": "Bundler",
      "target": "es2022",
      "lib": [
        "dom",
        "dom.iterable",
        "esnext"
      ],
      "skipLibCheck": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true,
      "isolatedModules": true,
      "noEmit": true,
      "jsx": "react",
      "typeRoots": ["./node_modules/@types", "./types"]
    }
}
