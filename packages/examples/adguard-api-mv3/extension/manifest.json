{
    "declarative_net_request": {
        "rule_resources": [
            {
                "id": "ruleset_2",
                "enabled": false,
                "path": "filters/declarative/ruleset_2/ruleset_2.json"
            },
            {
                "id": "ruleset_3",
                "enabled": false,
                "path": "filters/declarative/ruleset_3/ruleset_3.json"
            },
            {
                "id": "ruleset_4",
                "enabled": false,
                "path": "filters/declarative/ruleset_4/ruleset_4.json"
            }
        ]
    },
    "manifest_version": 3,
    "name": "Sample AdGuard MV3 API",
    "short_name": "Sample AdGuard MV3 API",
    "author": "Adguard Software Ltd",
    "version": "0.0.1",
    "description": "Sample extension using Adguard MV3 API",
    "permissions": [
        "tabs",
        "declarativeNetRequest",
        "declarativeNetRequestFeedback",
        "scripting",
        "activeTab",
        "webRequest",
        "webNavigation",
        "storage",
        "unlimitedStorage",
        "scripting",
        "cookies",
        "userScripts"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "all_frames": true,
            "js": [
                "content-script.js"
            ],
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "match_about_blank": true,
            "run_at": "document_start"
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "/adguard/*"
            ],
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "use_dynamic_url": true
        }
    ]
}