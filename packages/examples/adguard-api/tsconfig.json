{
    "compilerOptions": {
      "module": "esnext",
      "moduleResolution": "bundler",
      "target": "es2022",
      "lib": [
        "dom",
        "dom.iterable",
        "esnext"
      ],
      "skipLibCheck": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true,
      "noEmit": true,
      "jsx": "react"
    }
}
