{
    "compilerOptions": {
        "moduleResolution": "node",
        "target": "ESNext",
        "module":"ESNext",
        "lib": ["esnext", "dom"],
        "strict": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "incremental": true,
        "resolveJsonModule": true,
    },
    "typeRoots": [
        "node_modules/@types",
    ],
    "include": [
        "src",
    ]
}
